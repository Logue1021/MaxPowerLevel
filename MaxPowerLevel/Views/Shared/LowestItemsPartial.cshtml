@model IEnumerable<Item>
@using Humanizer

@if(Model.Any())
{
<div class="row">
    <h1>@("Lowest Item".ToQuantity(Model.Count(), ShowQuantityAs.None))</h1>
</div>
<div class="row">
    @foreach(var item in Model)
    {
    <div class="col-auto">
        @await Html.PartialAsync("ItemPartial", item)
    </div>
    }
</div>
}
